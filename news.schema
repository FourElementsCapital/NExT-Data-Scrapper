--
-- Name: news; Type: TABLE; Schema: public;
--

CREATE TABLE news (
    id integer NOT NULL,
    created_at timestamp DEFAULT now(),
    title text,
    description text,
    author text,
    full_text text,
    source text,
    original_url text DEFAULT '',
    full_html text DEFAULT '',
    gs_score double precision,
    gs_magnitude double precision,
    article_timestamp datetime
);
